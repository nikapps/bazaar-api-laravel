    "keywords": ["CafeBazaar", "API", "Laravel", "Wrapper", "Cafebazaar Api", "In App Purchase", "iap"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Ali Borhani",
            "email": "aliborhani1@gmail.com"
        },
        {
            "name": "Hossein Moradgholi",
